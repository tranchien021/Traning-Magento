<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">

    <body>
        <referenceContainer name="main">
            <block class="Magento\Framework\View\Element\Template" name="skip_to_content" template="Magento_Theme::html/sliderheader.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="instruction_content" template="Magento_Theme::html/instructionPage.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="category_content" template="Magento_Theme::html/topcategory.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="dealofday_content" template="Magento_Theme::html/dealofday.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="advertising_content" template="Magento_Theme::html/advertising.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="bestseller_content" template="Magento_Theme::html/bestseller.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="brand_content" template="Magento_Theme::html/brand.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="news_content" template="Magento_Theme::html/news.phtml"></block>
            <block class="Magento\Framework\View\Element\Template" name="recentview_content" template="Magento_Theme::html/recentview.phtml"></block>
        </referenceContainer>
    </body>

</page>
